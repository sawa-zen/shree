{
  "compilerOptions": {
    "target": "es6",
    "module": "es2015",
    "lib": ["dom", "es2015"],
    "allowJs": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "experimentalDecorators": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "removeComments": true,
    "moduleResolution": "node",
    "esModuleInterop": true
  }
}
